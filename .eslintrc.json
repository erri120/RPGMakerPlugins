{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": "projects/**",
  "rules": {
    "linebreak-style": "off",
    "spaced-comment": "off",
    "camelcase": "off",
    "require-jsdoc": "off"
  }
}
